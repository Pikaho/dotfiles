# for UJML
snippet _t
	_trace("${1:# }");
snippet _ts
	_trace(_strcat("${1:# }", ${2:val}));
snippet function with parameters variables ret
	<function name="${1:funcname}" type="${2:string}" >
		<parameters>
			<var name="arg" type="int"/>
		</parameters>
		<variables>
			<var name="ret" type="int"/>
		</variables>
		<script>
			${3}
		</script>
		<return><eval>ret</eval></return>
	</function>
snippet function with parameters ret
	<function name="${1:funcname}" type="${2:string}" >
		<parameters>
			<var name="arg" type="int"/>
		</parameters>
		<script>
			${3}
		</script>
		<return><eval>ret</eval></return>
	</function>
snippet function with parameters
	<function name="${1:funcname}" type="void" >
		<parameters>
			<var name="arg" type="int"/>
		</parameters>
		<script>
			${2}
		</script>
	</function>
snippet function with ret
	<function name="${1:funcname}" type="void" >
		<script>
			${2}
		</script>
		<return><eval>ret</eval></return>
	</function>
snippet if
	if ( ${1:condition} ) {
		${2}
	}
snippet ifeq
	if (_eq( ${1:lhs}, ${2:rhs} )) {
		${3}
	}
snippet ifs
	if (_streq( "${1:lhs}", ${2:rhs} )) {
		${3}
	}
